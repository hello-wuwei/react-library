{
  "extends": ["./.eslintrc.json"],
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "rules": {
        "no-console": [
          "warn",
          {
            "allow": ["warn", "error"]
          }
        ],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "off"
      }
    }
  ]
}
